{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Cypress Homologação",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run-script", "cypress:open:homologacao"],
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "DEBUG": "cypress:*"
            }
        },
        {
            "name": "Debug Cypress Staging",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run-script", "cypress:open:staging"],
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "DEBUG": "cypress:*"
            }
        }
    ]
}